<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="6">  
            <mapping cell="C5">unit.unitCode</mapping>
        </section> 
        <loop startRow="7" endRow="7" items="itemsconstructions" var="itemsconstruction" varType="org.motekar.project.civics.archieve.assets.inventory.objects.ItemsConstruction">
            <section startRow="7" endRow="7">
                <mapping row="7" col="1">itemsconstruction.itemName</mapping>
                <mapping row="7" col="2">itemsconstruction.itemCode</mapping>
                <mapping row="7" col="3">itemsconstruction.regNumber</mapping>
                <mapping row="7" col="4">itemsconstruction.buildingCategory</mapping>
                <mapping row="7" col="5">itemsconstruction.raised</mapping>
                <mapping row="7" col="6">itemsconstruction.frameWork</mapping>
                <mapping row="7" col="7">itemsconstruction.buildingWide</mapping>
                <mapping row="7" col="8">itemsconstruction.addressLocation</mapping>
                <mapping row="7" col="9">itemsconstruction.documentDate</mapping>
                <mapping row="7" col="10">itemsconstruction.documentNumber</mapping>
                <mapping row="7" col="11">itemsconstruction.startDate</mapping>
                <mapping row="7" col="12">itemsconstruction.landStatus</mapping>
                <mapping row="7" col="13">itemsconstruction.landCode</mapping>
                <mapping row="7" col="14">itemsconstruction.fundingSource</mapping>
                <mapping row="7" col="15">itemsconstruction.price</mapping>
                <mapping row="7" col="16">itemsconstruction.description</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>
