<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="6">  
            <mapping cell="C5">unit.unitCode</mapping>
        </section> 
        <loop startRow="7" endRow="7" items="itemsbuildings" var="itemsbuilding" varType="org.motekar.project.civics.archieve.assets.inventory.objects.ItemsBuilding">
            <section startRow="7" endRow="7">
                <mapping row="7" col="1">itemsbuilding.itemName</mapping>
                <mapping row="7" col="2">itemsbuilding.itemCode</mapping>
                <mapping row="7" col="3">itemsbuilding.regNumber</mapping>
                <mapping row="7" col="4">itemsbuilding.conditionText</mapping>
                <mapping row="7" col="5">itemsbuilding.raised</mapping>
                <mapping row="7" col="6">itemsbuilding.frameworks</mapping>
                <mapping row="7" col="7">itemsbuilding.wide</mapping>
                <mapping row="7" col="8">itemsbuilding.address</mapping>
                <mapping row="7" col="9">itemsbuilding.documentDate</mapping>
                <mapping row="7" col="10">itemsbuilding.documentNumber</mapping>
                <mapping row="7" col="11">itemsbuilding.landWide</mapping>
                <mapping row="7" col="12">itemsbuilding.landState</mapping>
                <mapping row="7" col="13">itemsbuilding.landCode</mapping>
                <mapping row="7" col="14">itemsbuilding.acquitionWay</mapping>
                <mapping row="7" col="15">itemsbuilding.price</mapping>
                <mapping row="7" col="15">itemsbuilding.adminCost</mapping>
                <mapping row="7" col="15">itemsbuilding.totalPrice</mapping>
                <mapping row="7" col="16">itemsbuilding.description</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>
